	unaMecha golpearSueloDeConcretoCon: unTaladro.
	unaMecha = #Soft ifTrue: [ unTaladro romperMecha].
	unaMecha = #Widia ifTrue: [unTaladro  
		vecesQueGolpeoCapaActual = 3 ifTrue: [ unTaladro romperMecha].
		^unTaladro ].
	unaMecha = #Diamante ifTrue: [ unTaladro
		vecesQueGolpeoCapaActual even ifTrue: [ unTaladro capasDeSueloDebajo first bajaUnDecimoDeResistencia].

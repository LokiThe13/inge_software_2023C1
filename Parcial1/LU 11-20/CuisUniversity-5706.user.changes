!Perforadora methodsFor: 'scan' stamp: 'JFOE 5/8/2023 18:43:30' prior: 50604469!
calcularProfundidadDelSuelo
	
	| acc ix alturaTotal|

	acc := 0.
	ix := 1.
	
	[ix <= capasDeSueloDebajo size] whileTrue: [ | capa altura |
		capa := capasDeSueloDebajo at: ix.
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
		ix := ix + 1.		
	].
	alturaTotal := 0.
	capasDeSueloDebajo do: [ :capa| alturaTotal := alturaTotal + capa altura ]

	^ acc.
	
	 ! !
!Perforadora methodsFor: 'scan' stamp: 'JFOE 5/8/2023 18:44:27' prior: 50608187!
